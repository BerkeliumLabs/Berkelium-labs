{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "berkeliumlabs": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "projects/berkeliumlabs",
      "sourceRoot": "projects/berkeliumlabs/src",
      "prefix": "berkeliumlabs",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "outputPath": "dist/berkeliumlabs",
            "index": "projects/berkeliumlabs/src/index.html",
            "browser": "projects/berkeliumlabs/src/main.ts",
            "polyfills": ["zone.js"],
            "tsConfig": "projects/berkeliumlabs/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              {
                "glob": "**/*",
                "input": "projects/berkeliumlabs/public"
              }
            ],
            "styles": ["projects/berkeliumlabs/src/styles.scss"],
            "scripts": [],
            "baseHref": "./",
            "webWorkerTsConfig": "projects/berkeliumlabs/tsconfig.worker.json"
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kB",
                  "maximumError": "1MB"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "4kB",
                  "maximumError": "8kB"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": { "port": 6600 },
          "configurations": {
            "production": {
              "buildTarget": "berkeliumlabs:build:production"
            },
            "development": {
              "buildTarget": "berkeliumlabs:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": ["zone.js", "zone.js/testing"],
            "tsConfig": "projects/berkeliumlabs/tsconfig.spec.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              {
                "glob": "**/*",
                "input": "projects/berkeliumlabs/public"
              }
            ],
            "styles": ["projects/berkeliumlabs/src/styles.scss"],
            "scripts": [],
            "webWorkerTsConfig": "projects/berkeliumlabs/tsconfig.worker.json"
          }
        }
      }
    }
  },
  "cli": {
    "analytics": false
  }
}
